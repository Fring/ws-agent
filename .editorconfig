# EditorConfig.org configuration
root = true

[*]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true

[package.json]
indent_size = 2

[*.md]
## Markdown uses 2 or more whitespace at the end of a line to indicate a line break. Let's keep those.
trim_trailing_whitespace = false
